{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "width": 800,
    "height": 500,
    "title": "Earthquakes of magnitude 5.5 or large between Jan 2020 and 18 Oct 2020 around AUS",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url": "https://jiaying959.github.io/FIT3179Assignment2/data/custom.geo.json",
          "format": {"type": "topojson", "feature": "custom"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "https://jiaying959.github.io/FIT3179Assignment2/data/aroundAUS2020.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "longitude", "type": "quantitative"},
          "latitude": {"field": "latitude", "type": "quantitative"},
          "size": {
            "field": "mag",
            "type": "quantitative",
            "scale": {"domain": [4, 8]}
          },
          "color": {
            "field": "mag",
            "type": "quantitative",
            "scale": {"scheme": "reds"}
          }
        }
      }
    ]
  }